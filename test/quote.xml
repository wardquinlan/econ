<context>
  <panel label="T10Y3M"
         frequency="months">
         
    <chart label="10 Year Treasury Const Maturity - 3 Month Treasury Const Maturity"
           scale="log" 
           span="80">

      <series ref="SP500"/>
    </chart>

    <chart label="T10Y3M"
           ngridlines="6"
           scale="linear"
           span="20">

      <series ref="T10Y3M" color="Red"/>
      <series ref="INV" color="Yellow"/>
    </chart>
  </panel>

  <panel label="TBOND1" 
         frequency="months">

    <chart label="1 Year Treasury Const Maturity"
           scale="log"
           span="80">

      <series ref="SP500"/>
    </chart>

    <chart label="TBOND1"
           ngridlines="10"
           span="20">

      <series ref="TBOND1" color="Red"/>
      <series ref="FC" color="Blue"/>
    </chart>
  </panel>

  <panel label="VIX"
         frequency="months">

    <chart label="CBOE Volatility Index"
           scale="log"
           span="80">

      <series ref="SP500"/>
      <series ref="VOL" color="Yellow"/>
    </chart>

    <chart label="VIX"
           ngridlines="10"
           span="20">

      <series ref="VIX" color="Red"/>
    </chart>
  </panel>
</context>

